<section>
    <h2>Type: <%= joke.type %></h2>
    <h2>Setup: <%= joke.setup %></h2>
    <h2>Punchline: <%= joke.punchline %></h2>
</section>

<form action="/favorite/<%= joke.id %>?_method=PUT" method="POST">
    <input  name="id" value="<%= joke.id %>">
    <input  name="type" value="<%= joke.type %>">
    <input  name="setup" value="<%= joke.setup %>">
    <input  name="punchline" value="<%= joke.punchline %>">
    <button>Update</button>
</form>

<form action="/favorite/<%= joke.id %>?_method=DELETE" method="POST">
    <button>DELETE</button>
</form>